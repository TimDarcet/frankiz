<?xml version="1.0" encoding="Latin-1"?>
<skin>
	<nom>Skin CSS</nom>
	<description>Skin entièrement skinable par css (à base de balises div).</description>
	<parametre id="sommaire">
		<description>Afficher le sommaire des annonces</description>
		<valeur id="trie">Trié</valeur>
		<valeur id="pas_trie">Non trié</valeur>
		<valeur id="cache">Ne pas afficher</valeur>
	</parametre>
	<parametre id="trier_annonces">
		<description>Trier les annonces</description>
		<valeur id="trie">Oui</valeur>
		<valeur id="pas_trie">Non</valeur>
	</parametre>
</skin>